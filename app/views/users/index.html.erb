<p id="notice"><%= notice %></p>

<h1>ユーザー一覧</h1>
<p>従業員の詳細です。</p>

<table>
  <thead>
    <tr>
      <th>name</th>
      <th>年齢</th>
      <th colspan="3">app</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= link_to 'ユーザー', user %></td>
        <td><%= link_to '編集', edit_user_path(user) %></td>
        <td><%= link_to '削除', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
